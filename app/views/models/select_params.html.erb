<%= render "details" %>

<div class="row">
  <div class="box box-solid">
    <div class="box-header">
      <i class="fa fa-list"></i><h3 class="box-title">Yay, digo pew</h3>
    </div>
    <div class="box-body">
       <table>
            <tr>
                <th> Key </th>
                <th> Value </th>
            </tr>
            
            <% @params.each do |key, value| %>
                <tr>
                    <td><%= key %></td>
                    <td><%= value %></td>
                </tr>
            <% end %>
        </table>
    </div>
 </div>
</div>
